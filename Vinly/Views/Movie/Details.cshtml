@model Vinly.Models.Movie
@{
                /**/

                ViewBag.Title = Model.Name;
                Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.MovieGenre.Name</li>
    <li>Release Date: @Model.ReleaseDate.ToLongDateString()</li>
    <li>Date Added: @Model.AddedDate.ToLongDateString()</li>
    <li>Number in Stock: @Model.NumberInStock</li>
</ul>